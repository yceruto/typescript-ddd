{
  "name": "typescript-ddd",
  "version": "1.0.0",
  "description": "DDD workshop with TypeScript",
  "main": "dist/src/main.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npx tsc",
    "start:dev": "npx tsc && nodemon --watch 'src/**/*.ts' --watch 'lib/**/*.ts' --exec 'ts-node -r tsconfig-paths/register' src/main.ts",
    "prepare": "ts-patch install",
    "test": "jest"
  },
  "keywords": [],
  "author": "Yonel Ceruto <yonelceruto@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "0http": "^3.5.2",
    "typia": "^4.1.16"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/supertest": "^2.0.12",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-patch": "^3.0.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  }
}
